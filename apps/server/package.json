{
  "name": "@smart-pocket/server",
  "version": "0.1.1",
  "private": true,
  "description": "Smart Pocket Server - Node.js backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec node src/index.js",
    "start": "node src/index.js",
    "build": "echo 'Build step - copy src to dist for now'",
    "migrate": "node -e \"require('./src/database/migrations').runMigrations().then(() => process.exit(0)).catch(e => { console.error(e); process.exit(1); })\"",
    "db:test": "node -e \"require('./src/database/migrations').testConnection().then(() => process.exit(0)).catch(() => process.exit(1))\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:openapi": "jest openapi-validation.test.js",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@actual-app/api": "catalog:",
    "bcrypt": "catalog:",
    "cors": "catalog:",
    "dinero.js": "catalog:",
    "dotenv": "catalog:",
    "express": "catalog:",
    "googleapis": "catalog:",
    "helmet": "catalog:",
    "jsonwebtoken": "catalog:",
    "openai": "catalog:",
    "pg": "catalog:",
    "swagger-ui-express": "catalog:",
    "uuid": "catalog:",
    "winston": "catalog:",
    "yamljs": "catalog:"
  },
  "devDependencies": {
    "jest": "catalog:",
    "js-yaml": "catalog:",
    "nodemon": "catalog:",
    "supertest": "catalog:"
  }
}
