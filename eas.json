{
  "cli": {
    "version": ">= 7.0.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "quality": {
      "channel": "quality",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "quality"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "channel": "production",
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "production"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccount": "./deploy/secrets/google-play-service-account.json",
        "track": "production"
      }
    }
  }
}
