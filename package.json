{
  "name": "smart-pocket",
  "version": "0.1.1",
  "private": true,
  "description": "Smart Pocket - Personal finance management with OCR receipt scanning",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server:dev": "pnpm --filter @smart-pocket/server dev",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test -- --coverage",
    "postman:generate": "node scripts/generate-postman.js",
    "api:generate": "orval",
    "mobile": "pnpm --filter @smart-pocket/mobile start",
    "mobile:ios": "pnpm --filter @smart-pocket/mobile ios",
    "mobile:android": "pnpm --filter @smart-pocket/mobile android",
    "mobile:web": "pnpm --filter @smart-pocket/mobile web",
    "mobile:start": "./scripts/start-expo.sh",
    "mobile:stop": "./scripts/stop-expo.sh",
    "mobile:check": "./scripts/check-expo.sh",
    "docker:dev": "./deploy/scripts/dev.sh",
    "docker:quality": "./deploy/scripts/quality.sh",
    "docker:prod": "./deploy/scripts/prod.sh",
    "docker:test": "./deploy/scripts/test.sh",
    "docker:build": "./deploy/scripts/build.sh",
    "docker:migrate": "./deploy/scripts/migrate.sh",
    "test:api": "./deploy/scripts/test-api.sh"
  },
  "keywords": [
    "finance",
    "budget",
    "ocr",
    "receipt-scanner"
  ],
  "author": "Smart Pocket",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "catalog:",
    "@babel/preset-flow": "catalog:",
    "@babel/preset-react": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@testing-library/react-native": "catalog:",
    "@types/jest": "catalog:",
    "babel-jest": "catalog:",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "js-yaml": "catalog:",
    "orval": "^7.17.2",
    "pnpm": "catalog:",
    "react-test-renderer": "catalog:"
  }
}
