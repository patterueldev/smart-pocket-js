{
  "name": "smart-pocket",
  "version": "0.1.1",
  "private": true,
  "description": "Smart Pocket - Personal finance management with OCR receipt scanning",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @smart-pocket/server dev",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test -- --coverage",
    "postman:generate": "node scripts/generate-postman.js",
    "docker:dev": "./deploy/scripts/dev.sh",
    "docker:quality": "./deploy/scripts/quality.sh",
    "docker:prod": "./deploy/scripts/prod.sh",
    "docker:test": "./deploy/scripts/test.sh",
    "docker:build": "./deploy/scripts/build.sh",
    "docker:migrate": "./deploy/scripts/migrate.sh",
    "test:api": "./deploy/scripts/test-api.sh"
  },
  "keywords": [
    "finance",
    "budget",
    "ocr",
    "receipt-scanner"
  ],
  "author": "Smart Pocket",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "js-yaml": "^4.1.1",
    "pnpm": "^8.0.0",
    "react-test-renderer": "^19.2.3"
  }
}
