{
  "name": "smart-pocket",
  "version": "0.2.3",
  "buildNumber": 10,
  "private": true,
  "description": "Smart Pocket - Personal finance management with OCR receipt scanning",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "server:dev": "node scripts/log-command.js server:dev && pnpm --filter @smart-pocket/server dev",
    "test": "pnpm -r test",
    "test:coverage": "pnpm -r test -- --coverage",
    "postman:generate": "node scripts/generate-postman.js",
    "api:generate": "orval",
    "mobile": "node scripts/log-command.js mobile && APP_VARIANT=development pnpm --filter @smart-pocket/mobile start",
    "mobile:quality": "node scripts/log-command.js mobile:quality && APP_VARIANT=quality pnpm --filter @smart-pocket/mobile start",
    "mobile:prod": "node scripts/log-command.js mobile:prod && APP_VARIANT=production pnpm --filter @smart-pocket/mobile start",
    "mobile:ios": "node scripts/log-command.js mobile:ios && pnpm --filter @smart-pocket/mobile ios",
    "mobile:android": "node scripts/log-command.js mobile:android && pnpm --filter @smart-pocket/mobile android",
    "mobile:web": "node scripts/log-command.js mobile:web && pnpm --filter @smart-pocket/mobile web",
    "mobile:start": "node scripts/log-command.js mobile:start && ./scripts/start-expo.sh",
    "mobile:stop": "node scripts/log-command.js mobile:stop && ./scripts/stop-expo.sh",
    "mobile:check": "node scripts/log-command.js mobile:check && ./scripts/check-expo.sh",
    "build:mobile:dev": "node scripts/log-command.js build:mobile:dev && pnpm --filter @smart-pocket/mobile build:dev",
    "build:mobile:qa": "node scripts/log-command.js build:mobile:qa && pnpm --filter @smart-pocket/mobile build:qa",
    "build:mobile:prod": "node scripts/log-command.js build:mobile:prod && pnpm --filter @smart-pocket/mobile build:prod",
    "docker:dev": "node scripts/log-command.js docker:dev && ./deploy/scripts/dev.sh",
    "docker:quality": "node scripts/log-command.js docker:quality && ./deploy/scripts/quality.sh",
    "docker:prod": "node scripts/log-command.js docker:prod && ./deploy/scripts/prod.sh",
    "docker:test": "node scripts/log-command.js docker:test && ./deploy/scripts/test.sh",
    "docker:dev:rebuild": "node scripts/log-command.js docker:dev:rebuild && bash ./deploy/scripts/rebuild-dev.sh",
    "docker:rebuild": "node scripts/log-command.js docker:rebuild && bash ./deploy/scripts/rebuild-dev.sh",
    "docker:build": "node scripts/log-command.js docker:build && ./deploy/scripts/build.sh",
    "docker:migrate": "node scripts/log-command.js docker:migrate && ./deploy/scripts/migrate.sh",
    "test:api": "node scripts/log-command.js test:api && ./deploy/scripts/test-api.sh",
    "api:generate": "node scripts/log-command.js api:generate && orval"
  },
  "keywords": [
    "finance",
    "budget",
    "ocr",
    "receipt-scanner"
  ],
  "author": "Smart Pocket",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "catalog:",
    "@babel/preset-flow": "catalog:",
    "@babel/preset-react": "catalog:",
    "@babel/preset-typescript": "catalog:",
    "@testing-library/react-native": "catalog:",
    "@types/jest": "catalog:",
    "babel-jest": "catalog:",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "js-yaml": "catalog:",
    "orval": "^7.17.2",
    "pnpm": "catalog:",
    "react-test-renderer": "catalog:"
  }
}
